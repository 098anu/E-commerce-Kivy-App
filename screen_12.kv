                            icon:'long-arrow-right'
                                    
                            size_hint:1,None
                    MDFloatingActionButton:
                        icon:'collection-item-4'
                        size_hint:None,None
                                
                        height:40
                        width:40
                BoxLayout:
                    size_hint_y:0.9
                    orientation:'vertical'
                    padding:20,10
                    MDFlatButton:
                        text:'''Comapny \nName'''
                        color:0,0,0,1
                        size_hint:0.1,0.7
                    BoxLayout:
                        size_hint_y:0.6
                    Label:
                        text:"        Hi User, start your journey\nin India's largest online market place"
                        color:0,0,0,1
                        markup:True
                    SingleLineTextField:
                        hint_text:'Company Name'
                        pos_hint:{"center_x":0.5,"center_y":0.5}
                        size_hint_x:0.8
                    BoxLayout:
                        size_hint_y:0.7
                    MDFillRoundFlatButton:
                        pos_hint:{"center_x":0.5,"center_y":0.5}
                        text:'NEXT'
                        color:1,1,1,1
                        size_hint:0.8,0.7
                        on_press:app.root.current='screen_13'
                            
                            
                BoxLayout:
                    size_hint_y:0.5
                            

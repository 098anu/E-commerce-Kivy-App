                    size_hint:1,None
                    height:40
                    padding:10,0
                            
                        
                    MDFloatingActionButton:
                                
                        icon:'collection-item-1'
                        id:fa1
                        background_normal:'cd24.jpg'
                        size_hint:None,None
                        height:40
                        width:40
                            
                    BoxLayout:
                        size_hint:None,None
                        width:1
                        height:40
                    BoxLayout:
                        size_hint:None,None
                        width:40
                        height:40
                                
                        NavigationDrawerIconButton:
                            icon:'long-arrow-right'
                            pos_hint:{"center_x":0.5,"center_y":0.5}
                            size_hint:1,None
                                        
                    MDFloatingActionButton:
                        icon:'collection-item-2'
                                
                        size_hint:None,None
                        height:40
                        width:40
                    BoxLayout:
                        size_hint:None,None
                        width:1
                        height:50
                    BoxLayout:
                        size_hint:None,None
                        width:40
                        height:40
                                
                        NavigationDrawerIconButton:
                            icon:'long-arrow-right'
                            pos_hint:{"center_x":0.5,"center_y":0.5}
                            size_hint:1,None
                    MDFloatingActionButton:
                        icon:'collection-item-3'
                        size_hint:None,None
                                
                        height:40
                        width:40
                    BoxLayout:
                        size_hint:None,None
                        width:1
                        height:40
